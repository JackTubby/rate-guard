{
  "name": "rate-guard",
  "version": "0.1.0-beta.2",
  "description": "Lightweight, configurable Express rate limiting middleware with multiple algorithm support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --watch",
    "test": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:performance": "jest tests/performance",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "rate-limiting",
    "express",
    "middleware",
    "rate-limiter",
    "ddos-protection",
    "api-throttling"
  ],
  "author": "Jack Tubby",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacktubby/rate-guard.git"
  },
  "homepage": "https://github.com/jacktubby/rate-guard#readme",
  "bugs": {
    "url": "https://github.com/jacktubby/rate-guard/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
